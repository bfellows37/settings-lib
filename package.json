{
  "name": "settings-lib",
  "description": "Simple library allowing override capability for application settings",
  "main": "./lib/settings.js",
  "version": "1.0.0",
  "author": "Joshua Thomas (http://github.com/brozeph)",
  "license": "MIT",
  "engines": {
    "node": ">=4.0"
  },
  "keywords": [
    "settings",
    "config",
    "override"
  ],
  "repository": "git://github.com/brozeph/settings-lib.git",
  "homepage": "http://github.com/brozeph/settings-lib",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@babel/runtime": "^7.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "eslint-plugin-babel": "^5.3.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-coveralls": "^0.1.4",
    "gulp-eslint": "^5.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0"
  },
  "scripts": {
    "lint": "gulp lint",
    "prepare": "gulp build",
    "prepublish": "check-node-version --npm \">=4\" || npm run prepare",
    "pretest": "gulp clean && gulp lint",
    "test": "nyc mocha ./test/src",
    "posttest": ""
  },
  "nyc": {
    "all": true,
    "include": [
      "src"
    ],
    "instrument": true,
    "report-dir": "./reports",
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": true
}
}
